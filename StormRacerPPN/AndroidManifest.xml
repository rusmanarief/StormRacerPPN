        <activity
            android:name=".CarDesign"
            android:label="@string/title_activity_cardesign" >
        </activity>
        <activity
            android:name=".Sponsors"
            android:label="@string/title_activity_sponsors" >
        </activity>
        <activity
            android:name=".ResultsStats"
            android:label="@string/title_activity_resultsstats" >
        </activity>
        <activity
            android:name=".Photos"
            android:label="@string/title_activity_photos" >
